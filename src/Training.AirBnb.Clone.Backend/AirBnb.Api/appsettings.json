{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "AvailabilitySettings": {
    "MinNights": 1,
    "MaxNights": 730,
    "PreparationMinDays": 1,
    "PreparationMaxDays": 2,
    "AvailabilityWindowMinValue": 3,
    "AvailabilityWindowMaxValue": 24
  },

  "ListingPropertyTypeSettings": {
    "MinFloorsCount": 1,
    "MaxFloorsCount": 185,
    "MinListingFloor": 0,
    "BuiltYearMinValue": 1900,
    "PropertySizeMinValue": 0
  },

  "ListingTypeSettings": {
    "MinListingTypeNameLength": 3,
    "MaxListingTypeNameLength": 100,
    "MinListingTypeDescriptionLength": 3,
    "MaxListingTypeDescriptionLength": 200
  },

  "ListingSettings": {
    "MinTitleLength": 3,
    "MaxTitleLength": 30,
    "MinListingPrice": 1,
    "ListingDescriptionMaxLength": 500
  },

  "ListingRulesSettings": {
    "GuestsMinCount": 1,
    "GuestsMaxCount": 50,
    "MinCheckInDurationInHours": 2,
    "FeatureMinValue": 0,
    "PropertyMinValue": 1
  },

  "ReviewSettings": {
    "RatingMinValue": 0,
    "RatingMaxValue": 5.0,
    "MaxCommentLength": 1000
  },

  "ListingRegistrationProgressSettings": {
    "ProgressMinValue": 1,
    "ProgressMaxValue": 9
  },

  "FileSettings": {
    "ListingImagePath": "wwwroot/Users/{{UserId}}/ListingImages/{{ListingId}}",
    "ProfilePicturePath": "wwwroot/Users/{{UserId}}/Profile"
  },

  "ImageSettings": {
    "MinImageSizeInKB": 50
  }
}